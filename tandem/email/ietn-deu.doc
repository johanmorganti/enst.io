\par {\pntext\pard\plain\s40 \f14\cgrid \loch\af14\dbch\af0\hich\f14 \'d8\tab}}\pard \s40\qj\fi-357\li357\sb120\nowidctlpar\widctlpar\jclisttab\tx360{\*\pn \pnlvlblt\ilvl0\ls6\pnrnot0\pnf14\pnstart1\pnindent360\pnhang{\pntxtb \'d8}}\ls6\adjustright {\fs24 die Sprache des jeweils anderen zu lernen,
